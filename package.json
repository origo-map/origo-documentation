{
  "name": "origo-documentation",
  "version": "2.9.0",
  "description": "documentation for origo",
  "main": "index.js",
  "scripts": {
    "start": "budo src/index.js --serve=bundle.js --live",
    "test": "npm run test-unit && eslint src && npm run prerender -- /dev/null && rm -rf lib",
    "test-unit": "mocha --compilers js:babel-register test",
    "prerender": "babel src --out-dir lib && node lib/render.js",
    "build": "browserify src/index.js | uglifyjs -c -m > bundle.js && npm run prerender -- index.html"
  },
  "browserify": {
    "transform": [
      "babelify",
      "brfs"
    ]
  },
  "keywords": [
    "origo",
    "documentation"
  ],
  "author": "Tom MacWright",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.4.0",
    "babel-eslint": "^10.1.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babelify": "^7.2.0",
    "brfs": "^1.4.2",
    "browserify": "^13.0.0",
    "cssnano": "^5.1.14",
    "es6-promise": "^3.0.2",
    "eslint": ">=4.18.2",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.14.0",
    "github-slugger": "^1.0.1",
    "highlight.js": "^10.6.0",
    "isomorphic-fetch": "^3.0.0",
    "lodash.debounce": "^4.0.3",
    "minifyify": "^7.1.0",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-pure-render": "^1.0.2",
    "remark": "^5.1.0",
    "remark-html": "^5.1.0",
    "remark-slug": "^5.1.2",
    "unist-util-select": "^4.0.3",
    "unist-util-visit": "^1.0.0"
  },
  "devDependencies": {
    "babel-register": "^6.3.13",
    "budo": "^11.8.4",
    "expect": "^1.13.4",
    "mocha": "^11.7.4",
    "to-vfile": "^1.0.0",
    "uglify-js": "^3.14.2"
  }
}
